{
  "name": "matecat/dqf-client",
  "type": "library",
  "description": "A simple Client for DQF written in PHP",
  "keywords": ["dqf", "client", "php"],
  "homepage": "https://github.com/matecat/dqf-client",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/matecat/dqf-client/issues"
  },
  "authors": [
    {
      "name": "Mauro Cassani",
      "email": "maurocassani1978@gmail.com",
      "homepage": "https://github.com/mauretto78"
    }
  ],
  "autoload": {
    "psr-4": {
      "Matecat\\Dqf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Matecat\\Dqf\\Tests\\": "tests/" }
  },
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.3",
    "monolog/monolog": "^1.0",
    "shrikeh/teapot": "^2.3",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "phpstan/phpstan": "^0.11.12",
    "friendsofphp/php-cs-fixer": "^2.15",
    "ramsey/uuid": "^3.8"
  }
}
